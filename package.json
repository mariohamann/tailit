{
  "name": "vite-lit-ts-starter",
  "version": "0.0.0",
  "main": "dist/my-element.es.js",
  "exports": {
    ".": "./dist/my-element.es.js"
  },
  "types": "types/my-element.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "dev": "vite --open",
    "dev:docs": "vitepress dev docs",
    "dev:cem": "cem analyze --litelement --globs 'src/components/**/!(*.stories).ts' --watch",
    "serve:docs": "vitepress serve docs  --port 8080",
    "build": "tsc && npm run build:cem && npm run build:www && npm run build:lib",
    "build:docs": "vitepress build docs",
    "build:www": "vite build",
    "build:lib": "vite build -c vite.lib.config.ts",
    "build:cem": "cem analyze --litelement --globs 'src/components/**/!(*.stories).ts'",
    "preview": "vite build && vite preview --open"
  },
  "dependencies": {
    "@babel/types": "^7.17.0",
    "@mariohamann/tailwindcss-var": "^0.1.1",
    "@vitejs/plugin-vue": "^2.3.1",
    "lit": "^2.2.2",
    "rollup-plugin-minify-html-literals": "^1.2.6",
    "rollup-plugin-terser": "^7.0.2",
    "tailwindcss": "^3.0.24",
    "vite": "^2.9.5",
    "vitepress": "^0.22.3"
  },
  "devDependencies": {
    "@algolia/client-search": "^4.13.0",
    "@custom-elements-manifest/analyzer": "^0.5.7",
    "@types/react": "17.0.44",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "api-viewer-element": "^1.0.0-pre.3",
    "autoprefixer": "^10.4.4",
    "concurrently": "^7.1.0",
    "cssnano": "^5.1.7",
    "eslint": "^8.13.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-lit": "^1.6.1",
    "eslint-plugin-wc": "^1.3.2",
    "lit-html": "^2.2.2",
    "postcss": "^8.4.12",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rollup": "^2.70.2",
    "typescript": "^4.6.3",
    "vue": "^3.2.33"
  },
  "customElements": "custom-elements.json"
}
